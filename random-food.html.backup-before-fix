        }
        
        function showFullRecipe(foodName) {
            // ËøôÈáåÂèØ‰ª•ÈìæÊé•Âà∞ÂÆåÊï¥ÁöÑÈ£üË∞±È°µÈù¢ÊàñÊòæÁ§∫ËØ¶ÁªÜÊ≠•È™§
            alert(`Full recipe for ${foodName} will be available soon!`);
        }
        
        function getCategoryName(category) {
            const categoryNames = {
                'all': 'All Foods',
                'chinese': 'Chinese',
                'western': 'Western',
                'dessert': 'Dessert',
                'drink': 'Drinks'
            };
            return categoryNames[category] || 'Unknown Category';
        }
        
        function getMealTypeName(mealType) {
            const mealTypeNames = {
                'breakfast': 'Breakfast',
                'lunch': 'Lunch',
                'dinner': 'Dinner',
                'snack': 'Snack',
                'dessert': 'Dessert',
                'drink': 'Drink'
            };
            return mealTypeNames[mealType] || 'Unknown Meal Type';
        }
        
        function getLifestyleName(lifestyle) {
            const lifestyleNames = {
                'vegetarian': 'Vegetarian',
                'non-vegetarian': 'Non-Vegetarian'
            };
            return lifestyleNames[lifestyle] || 'Unknown Lifestyle';
        }
        
