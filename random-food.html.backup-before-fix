        function generateRandomFood() {
            const mealType = document.getElementById('mealType').value;
            const lifestyle = document.getElementById('lifestyle').value;
            
            let availableFoods = Object.keys(foodDatabase);
            
            // Ê†πÊçÆÈ§êÁÇπÁ±ªÂûãËøáÊª§
            if (mealType !== 'random') {
                availableFoods = availableFoods.filter(food => 
                    foodDatabase[food].mealType === mealType
                );
            }
            
            // Ê†πÊçÆÁîüÊ¥ªÊñπÂºèËøáÊª§
            if (lifestyle !== 'random') {
                availableFoods = availableFoods.filter(food => 
                    foodDatabase[food].lifestyle === lifestyle
                );
            }
            
            if (availableFoods.length === 0) {
                alert('No food options available for the selected criteria! Please try different options.');
                return;
            }
            
            const randomFood = availableFoods[Math.floor(Math.random() * availableFoods.length)];
            const resultElement = document.getElementById('food-result');
            
            const foodInfo = foodDatabase[randomFood];
            
            // ÁîüÊàêÈÖçÊñôÂàóË°®
            const ingredients = generateIngredients(randomFood, foodInfo);
            
            // Áõ¥Êé•‰ΩøÁî®ÈùôÊÄÅÂõæÁâáURL
            resultElement.innerHTML = `
                <div class="food-result-card">
                    <div class="food-header">
                        <div class="food-info">
                            <div class="food-name-result">${randomFood}</div>
                            <div class="food-tags">
                                <span class="food-tag">${getLifestyleName(foodInfo.lifestyle)}</span>
                                <span class="food-tag">${getMealTypeName(foodInfo.mealType)}</span>
                                <span class="food-tag">Prep ${foodInfo.prepTime}</span>
                            </div>
                        </div>
                    </div>
                    <div class="food-details">
                        <div class="detail-section">
                            <div class="detail-title ingredients">Ingredients</div>
                            <ul class="ingredients-list">
                                ${ingredients.map(ingredient => `<li>${ingredient}</li>`).join('')}
                            </ul>
                        </div>
                        <div class="detail-section">
                            <div class="detail-title nutrition">Nutrition (per serving)</div>
                            <div class="nutrition-grid">
                                <div class="nutrition-item">
                                    <span class="nutrition-label calories">Calories:</span>
                                    <span class="nutrition-value">${foodInfo.nutrition.calories} kcal</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label fat">Fat Content:</span>
                                    <span class="nutrition-value">${foodInfo.nutrition.fat}g</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Saturated Fat:</span>
                                    <span class="nutrition-value">${Math.round(foodInfo.nutrition.fat * 0.3)}g</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Cholesterol:</span>
                                    <span class="nutrition-value">${Math.round(foodInfo.nutrition.fat * 5)}mg</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Sodium:</span>
                                    <span class="nutrition-value">${Math.round(foodInfo.nutrition.calories * 2)}mg</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Carbohydrates:</span>
                                    <span class="nutrition-value">${foodInfo.nutrition.carbs}g</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Fiber:</span>
                                    <span class="nutrition-value">${Math.round(foodInfo.nutrition.carbs * 0.1)}g</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Sugar:</span>
                                    <span class="nutrition-value">${Math.round(foodInfo.nutrition.carbs * 0.3)}g</span>
                                </div>
                                <div class="nutrition-item">
                                    <span class="nutrition-label">Protein:</span>
                                    <span class="nutrition-value">${foodInfo.nutrition.protein}g</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            `;
            
            // Add animation effect
            resultElement.style.animation = 'fadeIn 0.5s ease-in-out';
            setTimeout(() => {
                resultElement.style.animation = '';
            }, 500);
        }
        
        function generateIngredients(foodName, foodInfo) {
            // Ê†πÊçÆÈ£üÁâ©Á±ªÂûãÁîüÊàêÈÖçÊñôÂàóË°®
            const baseIngredients = {
                'chinese': [
                    '2 tablespoons soy sauce',
                    '1 tablespoon sesame oil',
                    '2 cloves garlic, minced',
                    '1 inch ginger, grated',
                    '2 green onions, chopped',
                    '1 tablespoon cornstarch',
                    'Salt and pepper to taste'
                ],
                'western': [
                    '2 tablespoons olive oil',
                    '1 onion, diced',
                    '2 cloves garlic, minced',
                    '1 teaspoon dried herbs',
                    'Salt and pepper to taste',
                    '1 cup vegetable broth',
                    '2 tablespoons butter'
                ],
                'dessert': [
                    '1 cup all-purpose flour',
                    '1/2 cup sugar',
                    '2 eggs',
                    '1/2 cup milk',
                    '1/4 cup butter, melted',
                    '1 teaspoon vanilla extract',
                    '1/2 teaspoon baking powder'
                ],
                'drink': [
                    '1 cup base liquid',
                    '2 tablespoons sweetener',
                    '1/2 cup ice cubes',
                    'Fresh herbs for garnish',
                    'Citrus slices for garnish'
                ]
            };
            
            const category = foodInfo.category;
            const ingredients = baseIngredients[category] || baseIngredients['western'];
            
            // Ê∑ªÂä†ÁâπÂÆöÈ£üÁâ©ÁöÑ‰∏ªË¶ÅÈÖçÊñô
            const mainIngredients = getMainIngredients(foodName, foodInfo);
            
            return [...mainIngredients, ...ingredients.slice(0, 3)];
        }
        
